{
  "name": "font-pack",
  "version": "0.1.0",
  "author": "zzzgydi",
  "license": "MIT",
  "description": "A font tool helps to convert ttf to woff and woff2",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "font-pack": "dist/cli.js"
  },
  "keywords": [
    "font",
    "ttf",
    "woff",
    "woff2",
    "minify",
    "convertor"
  ],
  "scripts": {
    "dev": "node build.js -w",
    "build": "node build.js && build:type",
    "build:type": "tsc --outfile dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzgydi/font-pack"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/pako": "^1.0.3",
    "cac": "^6.7.12",
    "esbuild": "^0.14.11",
    "fonteditor-core": "^2.1.8",
    "fs-extra": "^10.0.0",
    "pako": "^2.0.4"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "dist"
  ]
}
